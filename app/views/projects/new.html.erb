<h1>Add a new project</h1>

<%= form_for @project do |f| %>
  <div class="field">
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>

  <div class="field">
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>

  <div class="field">
    <%= f.label :github_link, 'GitHub' %>
    <%= f.text_field :github_link %>
  </div>

  <div class="field">
    <%= f.submit "Submit project" %>
  </div>
<% end %>
